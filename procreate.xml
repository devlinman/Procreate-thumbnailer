<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-procreate">
        <comment>Procreate artwork</comment>
        <comment xml:lang="en">Procreate artwork file</comment>
        <magic priority="50">
            <match value="PK" type="string" offset="0"/>
        </magic>
        <glob pattern="*.procreate"/>
    </mime-type>
</mime-info>
